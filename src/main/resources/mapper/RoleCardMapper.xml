<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

    <mapper namespace="com.mylearn.agentgate.mapper.RoleCardMapper">


    <select id="selectById" resultType="com.mylearn.agentgate.core.entity.RoleCard" parameterType="int">
        select id, name, user_id, prompt, create_time, update_time
        from tb_role_card
        where id = #{id}
    </select>


</mapper>